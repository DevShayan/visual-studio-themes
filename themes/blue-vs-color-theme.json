	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#008000"
			}
		},
		{
			"name": "Keyword, Storage, language constants, null, builtin function, Annotation, magic function",
			"scope": [
				"keyword",
				"keyword.operator.instanceof",
				"variable.language",
				"storage",
				"constant.language",
				"constant.language.NULL",
				"support.function.builtin",
				"storage.type.annotation",
				"punctuation.definition.annotation",
				"support.function.magic"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#a31515"
			}
		},
		{
			"name": "Classes, Namespaces",
			"scope": [
				"entity.name.type.class",
				"entity.name.type.struct",
				"entity.name.scope-resolution"
			],
			"settings": {
				"foreground": "#2b91af"
			}
		},
		{
			"name": "operators",
			"scope": [
				"punctuation.definition.directive.cpp",
				"punctuation.definition.constant",
				"keyword.operator",
				"punctuation.definition.keyword",
				"storage.type.function.arrow",
				"punctuation.definition.variable"
			],
			"settings": {
				"foreground": "#000000" // unnecessary
			}
		},
		{
			"name": "Properties (aka: keys)",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#2b91af"
			}
		},
		{
			"name": "Values",
			"scope": [
				"string.quoted.double.html",
				"string.quoted.double.xml"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "C++: directives",
			"scope": [
				"keyword.control.directive",
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "C++: function, preprocessor function, constructor, template type, namespaces",
			"scope": [
				"entity.name.function.definition.cpp",
				"entity.name.function.call.cpp",
				"entity.name.function.member.cpp",
				"entity.name.function.constructor.cpp",
				"entity.name.function.preprocessor",
				"entity.name.type.template",
				"entity.name.namespace"
			],
			"settings": {
				"foreground": "#2b91af"
			}
		},
		{
			"name": "HTML: attribute",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"name": "HTML: tag name",
			"scope": [
				"meta.tag.metadata",
				"meta.tag.structure"
			],
			"settings": {
				"foreground": "#800000"
			}
		},
		{
			"name": "HTML: defaults",
			"scope": [
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "XML: preprocessor, tag name",
			"scope": [
				"meta.tag.preprocessor.xml",
				"entity.name.tag.localname"
			],
			"settings": {
				"foreground": "#800000"
			}
		},
		{
			"name": "CSS: Selector",
			"scope": [
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#800000"
			}
		},
		{
			"name": "CSS: id, class, pseudo class, Wildcard",
			"scope": [
				"entity.other.attribute-name.css",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.pseudo-class.css",
				"entity.name.tag.wildcard"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#008800"
			}
		},
		{
			"name": "CSS: @media, functions",
			"scope": [
				"keyword.control.at-rule",
				"meta.function.shape.css",
				"meta.function.misc.css"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "CSS: defaults",
			"scope": [
				"keyword.other.unit",
				"constant.numeric.css"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "JAVA: non-primitive data types",
			"scope": [
				"storage.type.java",
				"punctuation.bracket.angle.java"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "JAVA: javadoc keyword",
			"scope": [
				"keyword.other.documentation.javadoc"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "JAVA: function definition",
			"scope": [
				"meta.method.identifier.java"
			],
			"settings": {
				"foreground": "#2b91af"
			}
		},
		{
			"name": "JAVA: defaults",
			"scope": [
				"variable.parameter.java",
				"punctuation.separator.delimiter.java"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "JS: defaults",
			"scope": [
				"entity.name.type.instance.jsdoc",
				"variable.other.jsdoc"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Python: function definition, support function",
			"scope": [
				"entity.name.function.python",
				"support.type.python"
			],
			"settings": {
				"foreground": "#2b91af"
			}
		},
		{
			"name": "Markdown: Heading, image-inline",
			"scope": [
				"markup.heading",
				"string.other.link.description.markdown",
				"meta.image.inline.markdown"
			],
			"settings": {
				"foreground": "#2b91af"
			}
		},
		{
			"name": "Markdown: seperator",
			"scope": [
				"meta.separator.markdown"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Markdown: bold",
			"scope": [
				"markup.bold.markdown"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Bash: defaults",
			"scope": [
				"variable.other.normal.shell"
			],
			"settings": {
				"foreground": "#000000"
			}
		}
		
	]
}
