		"statusBarItem.hoverBackground": "#3d3d3d",
		"editorGroupHeader.tabsBackground": "#1f1f1f",
		"tab.inactiveBackground": "#2e2e2e",
		"tab.activeBackground": "#3d3d3d",
		"tab.activeBorderTop": "#7160e8",
		"tab.activeForeground": "#fafafa",
		"tab.inactiveForeground": "#b2b2b2",
		"tab.hoverForeground": "#fafafa",
		"terminal.foreground": "#fafafa",
		"terminal.ansiRed": "#cd3131",
		"terminal.ansiGreen": "#0dbc79",
		"terminal.ansiYellow": "#e5e510",
		"terminal.ansiBlue": "#2472c8",
		"terminal.ansiMagenta": "#bc3fbc",
		"terminal.ansiCyan": "#11a8cd",
		"terminal.ansiWhite": "#e5e5e5",
		"terminal.ansiBrightRed": "#f14c4c",
		"terminal.ansiBrightGreen": "#23d18b",
		"terminal.ansiBrightYellow": "#f5f543",
		"terminal.ansiBrightBlue": "#3b8eea",
		"terminal.ansiBrightMagenta": "#d670d6",
		"terminal.ansiBrightCyan": "#29b8db",
		"terminal.ansiBrightBlack": "#666666",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#57a64a"
			}
		},
		{
			"name": "Key, Storage, language constants, delete, null, builtin function",
			"scope": [
				"keyword",
				"keyword.operator.instanceof",
				"variable.language",
				"storage",
				"constant.language",
				"keyword.operator.delete",
				"constant.language.NULL",
				"support.function.builtin"
			],
			"settings": {
				"foreground": "#5c9cd6"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#d69d85"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"name": "Classes, Namespaces",
			"scope": [
				"entity.name.type.class",
				"entity.name.type.struct",
				"entity.name.scope-resolution"
			],
			"settings": {
				"foreground": "#4ec9b0"
			}
		},
		{
			"name": "operators",
			"scope": [
				"punctuation.definition.directive.cpp",
				"punctuation.definition.constant",
				"keyword.operator",
				"punctuation.definition.keyword",
				"storage.type.function.arrow",
				"punctuation.definition.variable"
			],
			"settings": {
				"foreground": "#b4b4b4"
			}
		},
		{
			"name": "Properties (aka: keys)",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#4ec9b0"
			}
		},
		{
			"name": "Values",
			"scope": [
				"string.quoted.double.html",
				"string.quoted.double.xml"
			],
			"settings": {
				"foreground": "#c8c8c8"
			}
		},
		{
			"name": "C++: directives",
			"scope": [
				"keyword.control.directive",
			],
			"settings": {
				"foreground": "#9b9b9b"
			}
		},
		{
			"name": "C++: function, preprocessor function, constructor",
			"scope": [
				"entity.name.function.definition.cpp",
				"entity.name.function.call.cpp",
				"entity.name.function.member.cpp",
				"entity.name.function.constructor.cpp",
				"entity.name.function.preprocessor"
			],
			"settings": {
				"foreground": "#4ec9b0"
			}
		},
		{
			"name": "C++: defaults",
			"scope": [
				"entity.name.type.parameter.cpp"
			],
			"settings": {
				"foreground": "#dcdcdc"
			}
		},
		{
			"name": "HTML: doctype",
			"scope": [
				"meta.tag.metadata.doctype"
			],
			"settings": {
				"foreground": "#5c9cd6"
			}
		},
		{
			"name": "HTML: attribute",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#92caf4"
			}
		},
		{
			"name": "HTML: tag name",
			"scope": [
				"meta.tag.metadata",
				"meta.tag.structure"
			],
			"settings": {
				"foreground": "#5c9cd6"
			}
		},
		{
			"name": "HTML: tag punctuation",
			"scope": [
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#b4b4b4"
			}
		},
		{
			"name": "XML: preprocessor, tag name",
			"scope": [
				"meta.tag.preprocessor.xml",
				"entity.name.tag.localname"
			],
			"settings": {
				"foreground": "#5c9cd6"
			}
		},
		{
			"name": "CSS: Selector",
			"scope": [
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#5c9cd6"
			}
		},
		{
			"name": "CSS: Units",
			"scope": [
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"name": "CSS: id, class, pseudo class, Wildcard",
			"scope": [
				"entity.other.attribute-name.css",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.pseudo-class.css",
				"entity.name.tag.wildcard"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#7ca5a0"
			}
		},
		{
			"name": "CSS: functions",
			"scope": [
				"meta.function.shape.css",
				"meta.function.misc.css"
			],
			"settings": {
				"foreground": "#5c9cd6"
			}
		},
		{
			"name": "CSS: @media",
			"scope": [
				"keyword.control.at-rule"
			],
			"settings": {
				"foreground": "#5c9cd6"
			}
		},
		{
			"name": "JAVA: Annotation",
			"scope": [
				"storage.type.annotation",
				"punctuation.definition.annotation"
			],
			"settings": {
				"foreground": "#5c9cd6"
			}
		},
		{
			"name": "JAVA: non-primitive data types",
			"scope": [
				"storage.type.java",
				"punctuation.bracket.angle.java"
			],
			"settings": {
				"foreground": "#5c9cd6"
			}
		},
		{
			"name": "JAVA: javadoc keyword",
			"scope": [
				"keyword.other.documentation.javadoc"
			],
			"settings": {
				"foreground": "#b4b4b4"
			}
		},
		{
			"name": "JAVA: function definition",
			"scope": [
				"meta.method.identifier.java"
			],
			"settings": {
				"foreground": "#4ec9b0"
			}
		},
		{
			"name": "JAVA: defaults",
			"scope": [
				"variable.parameter.java",
				"punctuation.separator.delimiter.java"
			],
			"settings": {
				"foreground": "#dcdcdc"
			}
		},
		{
			"name": "JS: defaults",
			"scope": [
				"entity.name.type.instance.jsdoc"
			],
			"settings": {
				"foreground": "#dcdcdc"
			}
		},
		{
			"name": "Python: magic function",
			"scope": [
				"support.function.magic.python"
			],
			"settings": {
				"foreground": "#5c9cd6"
			}
		},
		{
			"name": "Python: function definition, support function",
			"scope": [
				"entity.name.function.python",
				"support.type.python"
			],
			"settings": {
				"foreground": "#4ec9b0"
			}
		},
		{
			"name": "Markdown: Heading, image-inline",
			"scope": [
				"markup.heading",
				"string.other.link.description.markdown",
				"meta.image.inline.markdown"
			],
			"settings": {
				"foreground": "#4ec9b0"
			}
		},
		{
			"name": "Markdown: seperator",
			"scope": [
				"meta.separator.markdown"
			],
			"settings": {
				"foreground": "#5c9cd6"
			}
		},
		{
			"name": "Markdown: bold",
			"scope": [
				"markup.bold.markdown"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Bash: defaults",
			"scope": [
				"variable.other.normal.shell"
			],
			"settings": {
				"foreground": "#dcdcdc"
			}
		}
		
	]
}
